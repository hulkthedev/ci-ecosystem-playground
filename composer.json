{
  "name": "ci-ecosystem-playground",
  "version": "1.0",
  "type": "project",
  "license": "MIT",
  "description": "Simple playground for travis and sonarcloud",
  "authors": [{
    "name": "~albei",
    "email": "fatal.error.27@gmail.com",
    "role": "developer"
  }],
  "config": {
    "http-basic": {},
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~8"
  }
}

{
  "name": "ci_playground",
  "version": "1.0.0",
  "type": "project",
  "license": "MIT",
  "description": "Simple CI Playground*",
  "config": {
    "http-basic": {},
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~8"
  }
}
